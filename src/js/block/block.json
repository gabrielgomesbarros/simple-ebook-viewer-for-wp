{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "simple-ebook-viewer/ebook-viewer-block",
	"version": "0.1.0",
	"title": "Ebook Viewer Block",
	"category": "widgets",
	"description": "Display and navigate the contents of an ebook.",
	"example": {},
	"attributes": {
		"book": {
			"type": "string"
		}
	},
	"supports": {
		"dimensions": {
            "minHeight": true,
            "aspectRatio": true
		},
		"html": false
	},
	"textdomain": "simple-ebook-viewer",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
